,CAAA,KAAA,YAAA;AACA,KAbA;AAeA;AACA,IAAA,cAhBA,4BAgBA;AACA;AACA,UAAA,KAAA,QAAA,GAAA,CAAA,EACA,KAAA,OAAA,CAAA;AAAA,QAAA,IAAA,EAAA,MAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA,EADA,CAEA;AACA;AAHA,WAIA,KAAA,YAAA,GAAA,CAAA,KAAA,YAAA;AACA,KAvBA;;AAyBA;AACA,IAAA,gBA1BA,8BA0BA;AACA;AACA,UAAA,KAAA,QAAA,GAAA,CAAA,EACA,KAAA,OAAA,CAAA;AAAA,QAAA,IAAA,EAAA,MAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA,EADA,CAEA;AACA;AAHA,WAIA,KAAA,iBAAA,GAAA,CAAA,KAAA,iBAAA;AACA,KAjCA;;AAmCA;AACA;AACA,IAAA,SArCA,qBAqCA,CArCA,EAqCA;AACA,WAAA,YAAA,GAAA,CAAA;AACA,KAvCA;AAwCA,IAAA,cAxCA,0BAwCA,CAxCA,EAwCA;AACA,WAAA,iBAAA,GAAA,CAAA;AACA,KA1CA;;AA2CA;AACA;AACA,IAAA,aA7CA,yBA6CA,MA7CA,EA6CA;AACA,UAAA,MAAA,IAAA,CAAA,EAAA,OAAA,iBAAA;AACA,UAAA,MAAA,IAAA,CAAA,EAAA,OAAA,gBAAA;AACA,aAAA,QAAA;AACA,KAjDA;;AAkDA;AACA;AACA,IAAA,aApDA,yBAoDA,MApDA,EAoDA;AACA,UAAA,MAAA,IAAA,CAAA,EAAA,OAAA,IAAA;AACA,UAAA,MAAA,IAAA,CAAA,EAAA,OAAA,OAAA;AACA,KAvDA;;AAwDA;AACA,IAAA,WAzDA,uBAyDA,KAzDA,EAyDA;AACA;AACA,aAAA,KAAA,CAAA,QAAA,GAAA,OAAA,CAAA,yBAAA,EAAA,KAAA,CAAA;AACA,KA5DA;;AA6DA;AACA,IAAA,WA9DA,yBA8DA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MADA,GACA,CAAA,KAAA,CAAA,WAAA,GAAA,CAAA,IAAA,KAAA,CAAA,QAAA,GAAA,CADA,EAEA;;AAFA;AAAA,uBAGA,KAAA,CAAA,GAAA,CACA,yDACA,MADA,GAEA,SAFA,GAGA,KAAA,CAAA,QAHA,GAIA,cAJA,GAKA,KAAA,CAAA,QALA,GAMA,eANA,GAOA,KAAA,CAAA,gBAPA,GAQA,aARA,GASA,KAAA,CAAA,cATA,GAUA,eAVA,GAWA,KAAA,CAAA,aAXA,GAYA,mBAZA,GAaA,KAAA,CAAA,aAbA,GAcA,UAdA,GAeA,KAAA,CAAA,WAfA,GAgBA,UAhBA,GAiBA,KAAA,CAAA,WAlBA,CAHA;;AAAA;AAGA,gBAAA,QAHA;AAuBA,gBAAA,KAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;;AAvBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAwBA,KAtFA;;AAwFA;AACA,IAAA,QAzFA,sBAyFA;AACA,WAAA,WAAA,GAAA,KAAA,WAAA,GAAA,CAAA;AAEA;;AACA,WAAA,WAAA;AACA,KA9FA;;AAgGA;AACA,IAAA,QAjGA,sBAiGA;AACA,WAAA,WAAA,GAAA,KAAA,WAAA,GAAA,CAAA;AAEA,UAAA,KAAA,WAAA,GAAA,CAAA,EAAA,KAAA,WAAA,GAAA,CAAA;AACA,WAAA,WAAA;AACA,KAtGA;;AAwGA;AACA,IAAA,SAzGA,uBAyGA;AACA,WAAA,WAAA,GAAA,CAAA;AACA,WAAA,WAAA;AACA,KA5GA;;AA8GA;AACA,IAAA,MA/GA,oBA+GA;AACA,MAAA,QAAA,CAAA,MAAA;AACA;AACA;;AAlHA,GAPA;AA2HA,EAAA,QAAA,EAAA,CACA;AACA;AACA;AACA;AACA;AALA,GA3HA;AAkIA,EAAA,IAlIA,kBAkIA;AACA,WAAA;AACA,MAAA,OAAA,EAAA,EADA;AAEA,MAAA,YAAA,EAAA,EAFA;AAGA,MAAA,OAAA,EAAA,EAHA;AAIA,MAAA,QAAA,EAAA,EAJA;AAKA,MAAA,gBAAA,EAAA,EALA;AAMA,MAAA,aAAA,EAAA,EANA;AAOA,MAAA,aAAA,EAAA,EAPA;AAQA,MAAA,WAAA,EAAA,CARA;AASA,MAAA,WAAA,EAAA,CATA;AAUA,MAAA,cAAA,EAAA,CAVA;AAWA,MAAA,QAAA,EAAA,CAAA,CAXA;AAYA,MAAA,QAAA,EAAA,EAZA;AAaA,MAAA,WAAA,EAAA,CAbA;AAcA,MAAA,YAAA,EAAA,IAdA;AAeA,MAAA,iBAAA,EAAA,IAfA;AAgBA,MAAA,QAAA,EAAA,OAAA,CAAA,WAAA,EAhBA;AAiBA,MAAA,OAAA,EAAA,UAjBA;AAkBA,MAAA,cAAA,EAAA;AAlBA,KAAA;AAoBA,GAvJA;;AAwJA;AACA,EAAA,OAzJA,qBAyJA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,cAAA,MADA,GACA,CAAA,MAAA,CAAA,WAAA,GAAA,CAAA,IAAA,MAAA,CAAA,QAAA,GAAA,CADA;AAAA;AAAA,qBAEA,KAAA,CAAA,GAAA,CACA,yDACA,MADA,GAEA,SAFA,GAGA,MAAA,CAAA,QAHA,GAIA,gCALA,CAFA;;AAAA;AAEA,cAAA,QAFA;AASA,cAAA,MAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA;AAnKA,CAAA","sourcesContent":["<template>\r\n  <div class=\"contenBody\">\r\n    <!-- header của content  -->\r\n    <div class=\"contentHeader\">\r\n      <!-- nút thêm  -->\r\n      <button\r\n        title=\"Thêm mới cửa hàng\"\r\n        class=\"contentHeaderButton\"\r\n        @click=\"btnAddOnClick\"\r\n      >\r\n        <div class=\"iconHeader\">\r\n          <div class=\"iconAdd\"></div>\r\n        </div>\r\n        <div class=\"iconText\">Thêm mới</div>\r\n      </button>\r\n      <button\r\n        title=\"Nhân bản thêm một cửa hàng nữa\"\r\n        class=\"contentHeaderButton\"\r\n      >\r\n        <div class=\"iconHeader\">\r\n          <div class=\"iconDupplicate\"></div>\r\n        </div>\r\n        <div class=\"iconText\">Nhân bản</div>\r\n      </button>\r\n      <!-- sửa  -->\r\n      <button\r\n        title=\"Chỉnh sửa thông tin cửa hàng\"\r\n        class=\"contentHeaderButton\"\r\n        @click=\"btnEditOnClick\"\r\n      >\r\n        <div class=\"iconHeader\">\r\n          <div class=\"iconEdit\"></div>\r\n        </div>\r\n        <div class=\"iconText\">Sửa</div>\r\n      </button>\r\n      <!-- xóa  -->\r\n      <button\r\n        title=\"Xóa cửa hàng\"\r\n        class=\"contentHeaderButton\"\r\n        @click=\"btnDeleteOnClick\"\r\n      >\r\n        <div class=\"iconHeader\">\r\n          <div class=\"iconDelete\"></div>\r\n        </div>\r\n        <div class=\"iconText\">Xóa</div>\r\n      </button>\r\n      <Popup\r\n        :isHidePopup=\"isHidePopupParent\"\r\n        @outIsHidePopup=\"outIsHidePopup\"\r\n        :product=\"product\"\r\n      />\r\n      <!-- nạp  -->\r\n      <button class=\"contentHeaderButton\" @click=\"reload\">\r\n        <div class=\"iconHeader\">\r\n          <div class=\"iconImport\"></div>\r\n        </div>\r\n        <div class=\"iconText\">Nạp</div>\r\n      </button>\r\n    </div>\r\n\r\n    <div class=\"gridTable\">\r\n      <table class=\"tableContent\">\r\n        <thead>\r\n          <tr class=\"filter\">\r\n            <th width=\"4%\">\r\n              <input type=\"checkbox\" />\r\n            </th>\r\n            <th width=\"10%\">\r\n              SKU\r\n              <div class=\"filterField\">\r\n                <div class=\"iconSearch\">*</div>\r\n                <input\r\n                  class=\"searchField\"\r\n                  @keyup.enter=\"getPaginate\"\r\n                  v-model.trim=\"enterSKU\"\r\n                />\r\n              </div>\r\n            </th>\r\n            <th width=\"15%\">\r\n              Tên hàng hóa\r\n              <div class=\"filterField\">\r\n                <div class=\"iconSearch\">*</div>\r\n                <input\r\n                  class=\"searchField\"\r\n                  @keyup.enter=\"getPaginate\"\r\n                  v-model.trim=\"enterProductName\"\r\n                />\r\n              </div>\r\n            </th>\r\n            <th width=\"25%\">\r\n              Nhóm hàng hóa\r\n              <div class=\"filterField\">\r\n                <div class=\"iconSearch\">*</div>\r\n                <input\r\n                  class=\"searchField\"\r\n                  @keyup.enter=\"getPaginate\"\r\n                  v-model.trim=\"enterCategory\"\r\n                />\r\n              </div>\r\n            </th>\r\n            <th width=\"8%\">\r\n              Đơn vị tính\r\n              <div class=\"filterField\">\r\n                <div class=\"iconSearch\">*</div>\r\n                <input\r\n                  class=\"searchField\"\r\n                  @keyup.enter=\"getPaginate\"\r\n                  v-model.trim=\"enterUnitName\"\r\n                />\r\n              </div>\r\n            </th>\r\n            <th width=\"10%\">\r\n              Giá bán TB\r\n              <div class=\"filterField\">\r\n                <div class=\"iconSearch\">≥</div>\r\n                <input\r\n                  class=\"searchField\"\r\n                  @keyup.enter=\"getPaginate\"\r\n                  v-model.number=\"enterSellPrice\"\r\n                  style=\"text-align: right\"\r\n                />\r\n              </div>\r\n            </th>\r\n            <th width=\"15%\">\r\n              Hiển thị trên màn hình bán hàng\r\n              <div class=\"filterField\">\r\n                <select\r\n                  style=\"width: 100%; height: 38px\"\r\n                  v-model=\"enterIsShow\"\r\n                  @change=\"getPaginate\"\r\n                >\r\n                  <option value=\"2\" selected>Tất cả</option>\r\n                  <option value=\"1\">Có</option>\r\n                  <option value=\"0\">Không</option>\r\n                </select>\r\n              </div>\r\n            </th>\r\n            <th width=\"12%\">\r\n              Trạng thái\r\n              <div class=\"filterField\">\r\n                <select\r\n                  style=\"width: 100%; height: 38px\"\r\n                  v-model=\"enterStatus\"\r\n                  @change=\"getPaginate\"\r\n                >\r\n                  <option value=\"2\">Tất cả</option>\r\n                  <option value=\"1\">Đang kinh doanh</option>\r\n                  <option value=\"0\">Ngừng kinh doanh</option>\r\n                </select>\r\n              </div>\r\n            </th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr\r\n            class=\"tableRow\"\r\n            v-for=\"(product, index) in products\"\r\n            :key=\"index\"\r\n            :class=\"{ hightlight: isActive == index }\"\r\n            @click=\"rowOnClick(product, index)\"\r\n          >\r\n            <td>\r\n              <div class=\"cell\">\r\n                <input\r\n                  type=\"checkbox\"\r\n                  v-model=\"checked\"\r\n                  :value=\"product.productID\"\r\n                />\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <div class=\"cell\">\r\n                {{ product.sku }}\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <div class=\"cell\" id=\"productName\">\r\n                {{ product.productName }}\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <div class=\"cell\">\r\n                {{ product.categoryName }}\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <div class=\"cell\">\r\n                {{ product.unitName }}\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <div class=\"cell\" style=\"text-align: right; padding-right: 5px\">\r\n                {{ formatPrice(product.sellPrice) }}\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <div class=\"cell\">{{ convertIsShow(product.isShow) }}</div>\r\n            </td>\r\n            <td>\r\n              <div class=\"cell\">{{ convertStatus(product.status) }}</div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n    <!-- Component chi tiết  -->\r\n    <Details :isHide=\"isHideParent\" :product=\"product\" @outIsHide=\"outIsHide\" />\r\n    <div class=\"contentFooter\">\r\n      <div class=\"contentFooterLeft\">\r\n        <button class=\"buttonPage\" @click=\"firstPage\">\r\n          <div class=\"firstPage\"></div>\r\n        </button>\r\n        <button class=\"buttonPage\" @click=\"prevPage\">\r\n          <div class=\"prevPage\"></div>\r\n        </button>\r\n        <div class=\"contentFooterPaging\">\r\n          <a>Trang </a>\r\n          <input\r\n            type=\"text\"\r\n            class=\"currentPage\"\r\n            value=\"1\"\r\n            v-model.number=\"currentPage\"\r\n            @keyup.enter=\"getPaginate\"\r\n          />\r\n          <a> trên </a>\r\n          <div class=\"totalPage\">{{ typePage }}</div>\r\n        </div>\r\n        <button class=\"buttonPage\" @click=\"nextPage\">\r\n          <div class=\"nextPage\"></div>\r\n        </button>\r\n        <button class=\"buttonPage\">\r\n          <div class=\"lastPage\"></div>\r\n        </button>\r\n        <button class=\"buttonPage\" @click=\"reload\">\r\n          <div class=\"refresh\"></div>\r\n        </button>\r\n        <select class=\"typePage\" v-model=\"typePage\" @change=\"getPaginate\">\r\n          <option value=\"5\">5</option>\r\n          <option value=\"25\" selected>25</option>\r\n          <option value=\"50\">50</option>\r\n          <option value=\"100\">100</option>\r\n        </select>\r\n      </div>\r\n\r\n      <div class=\"contentFooterRight\">\r\n        <!-- todo proc cần trả về số bản ghi thỏa mãn và số trang thỏa mãn -->\r\n        <a>Hiển thị 1- {{ products.length }} trên {{ typePage }} kết quả</a>\r\n        <!-- <p>Các input đã check: {{ checked }}</p> -->\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as axios from \"axios\";\r\nimport service from \"@/data.js\";\r\nimport Details from \"./ProductListDetails.vue\";\r\nimport Popup from \"../popup/PopupDelete\";\r\nexport default {\r\n  name: \"Product\",\r\n  props: {},\r\n  components: {\r\n    Details,\r\n    Popup,\r\n  },\r\n  methods: {\r\n    /**Chọn dòng  */\r\n    rowOnClick(product, index) {\r\n      this.isActive = index;\r\n      this.product = product;\r\n    },\r\n    /**Mở form */\r\n    /**Mở form để thêm mới */\r\n    btnAddOnClick() {\r\n      // gán cho product bind lên là prodcut rỗng\r\n      this.product = this.productEmpty;\r\n      // Mở form\r\n      this.isHideParent = !this.isHideParent;\r\n    },\r\n\r\n    // Nút sửa\r\n    btnEditOnClick() {\r\n      //Nếu chưa active thì chọn\r\n      if (this.isActive < 0)\r\n        this.$notify({ type: \"warn\", text: \"Vui lòng chọn cửa hàng cần sửa\" });\r\n      // ngược lại thì mở form\r\n      //Createdby VTT 19/03/21\r\n      else this.isHideParent = !this.isHideParent;\r\n    },\r\n\r\n    /**Xóa hàng hóa */\r\n    btnDeleteOnClick() {\r\n      // Nếu chưa active thì thông báo chưa chọn\r\n      if (this.isActive < 0)\r\n        this.$notify({ type: \"warn\", text: \"Vui lòng chọn cửa hàng cần xóa\" });\r\n      //Ngược lại thì mở popup xóa\r\n      //Createdby VTT 19/03/21\r\n      else this.isHidePopupParent = !this.isHidePopupParent;\r\n    },\r\n\r\n    /**Đóng form */\r\n    //Createdby VTT 19/03/21\r\n    outIsHide(e) {\r\n      this.isHideParent = e;\r\n    },\r\n    outIsHidePopup(e) {\r\n      this.isHidePopupParent = e;\r\n    },\r\n    /**format trạng thái */\r\n    //Createdby VTT 19/03/21\r\n    convertStatus(status) {\r\n      if (status == 0) return \"Ngừng hoạt động\";\r\n      if (status == 1) return \"Đang hoạt động\";\r\n      return \"Tất cả\";\r\n    },\r\n    /**format hiển thị */\r\n    //Createdby VTT 19/03/21\r\n    convertIsShow(isShow) {\r\n      if (isShow == 1) return \"Có\";\r\n      if (isShow == 0) return \"Không\";\r\n    },\r\n    /**format tiền */\r\n    formatPrice(value) {\r\n      // let val = (value/1).toFixed(2).replace('.', ',')\r\n      return value.toString().replace(/(\\d)(?=(\\d{3})+(?!\\d))/g, \"$1.\");\r\n    },\r\n    /**API phân trang + filter */\r\n    async getPaginate() {\r\n      var offset = (this.currentPage - 1) * this.typePage + 1;\r\n      // var sellPriceConverted = this.enterSellPrice.convertSellPrice();\r\n      const response = await axios.get(\r\n        \"http://localhost:55810/api/Products/Paginate?offset=\" +\r\n          offset +\r\n          \"&limit=\" +\r\n          this.typePage +\r\n          \"&productSKU=\" +\r\n          this.enterSKU +\r\n          \"&productName=\" +\r\n          this.enterProductName +\r\n          \"&sellPrice=\" +\r\n          this.enterSellPrice +\r\n          \"&productUnit=\" +\r\n          this.enterUnitName +\r\n          \"&productCategory=\" +\r\n          this.enterCategory +\r\n          \"&isShow=\" +\r\n          this.enterIsShow +\r\n          \"&status=\" +\r\n          this.enterStatus\r\n      );\r\n      this.products = response.data;\r\n    },\r\n\r\n    /**Sang trang tiếp theo */\r\n    nextPage() {\r\n      this.currentPage = this.currentPage + 1;\r\n\r\n      /**Nếu this.currentPage > tối đa thì gán bằng tối đa */\r\n      this.getPaginate();\r\n    },\r\n\r\n    /**về trang trước */\r\n    prevPage() {\r\n      this.currentPage = this.currentPage - 1;\r\n\r\n      if (this.currentPage < 1) this.currentPage = 1;\r\n      this.getPaginate();\r\n    },\r\n\r\n    /**Về trang đầu */\r\n    firstPage() {\r\n      this.currentPage = 1;\r\n      this.getPaginate();\r\n    },\r\n\r\n    /**reload */\r\n    reload() {\r\n      location.reload();\r\n    },\r\n    /**API laays lenght */\r\n  },\r\n  computed: {\r\n    // formatInputPrice(enterSellPrice){\r\n    //   if(enterSellPrice < 0)\r\n    //   enterSellPrice = 0;\r\n    //   return enterSellPrice;\r\n    // }\r\n  },\r\n  data() {\r\n    return {\r\n      product: {},\r\n      productEmpty: {},\r\n      checked: [],\r\n      enterSKU: \"\",\r\n      enterProductName: \"\",\r\n      enterCategory: \"\",\r\n      enterUnitName: \"\",\r\n      enterStatus: 2,\r\n      enterIsShow: 2,\r\n      enterSellPrice: 0,\r\n      isActive: -1,\r\n      typePage: 25,\r\n      currentPage: 1,\r\n      isHideParent: true,\r\n      isHidePopupParent: true,\r\n      products: service.getProducts(),\r\n      allMode: \"allPages\",\r\n      checkBoxesMode: \"always\",\r\n    };\r\n  },\r\n  /**API ban đầu */\r\n  async created() {\r\n    var offset = (this.currentPage - 1) * this.typePage + 1;\r\n    const response = await axios.get(\r\n      \"http://localhost:55810/api/Products/Paginate?offset=\" +\r\n        offset +\r\n        \"&limit=\" +\r\n        this.typePage +\r\n        \"&sellPrice=0&isShow=2&status=2\"\r\n    );\r\n    this.products = response.data;\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.filter .filterField {\r\n  height: 46px;\r\n  width: 100%;\r\n  padding: 4px;\r\n  display: flex;\r\n}\r\n.iconSearch {\r\n  border: 1px solid #9e9e9e;\r\n  border-radius: 4px;\r\n  margin: 1px;\r\n  height: 100%;\r\n  width: 40px;\r\n  line-height: 40px;\r\n  text-align: center;\r\n  background-color: #ffffff;\r\n}\r\n.searchField {\r\n  margin: 1px;\r\n  height: 100%;\r\n  width: calc(100% - 40px);\r\n  border-radius: 4px;\r\n  border: 1px solid #9e9e9e;\r\n}\r\n.hightlight {\r\n  background-color: #dfe3e8;\r\n  border-left: 1px solid #dfe3e8;\r\n}\r\n/* .contenBody{} */\r\n\r\n.contentHeader {\r\n  height: 40px;\r\n  background-color: #2b3173;\r\n  position: relative;\r\n  display: flex;\r\n  justify-content: flex-start;\r\n  align-items: center;\r\n}\r\n\r\n.contentHeader .contentHeaderButton .iconText {\r\n  color: #ffffff;\r\n  margin-left: 5px;\r\n}\r\n\r\n.contentHeader .contentHeaderButton .iconHeader {\r\n  align-items: center;\r\n  justify-content: center;\r\n  padding: 3px;\r\n}\r\n\r\n.currentPage {\r\n  width: 45px;\r\n}\r\n.typePage {\r\n  width: 75px;\r\n}\r\na {\r\n  margin: 10px;\r\n}\r\n.totalPage {\r\n  width: 15px;\r\n}\r\n</style>"],"sourceRoot":"src/views/product"}]}