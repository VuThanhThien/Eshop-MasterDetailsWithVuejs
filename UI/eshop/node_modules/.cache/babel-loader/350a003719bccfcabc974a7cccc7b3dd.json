AAA,KAAA,iBAAA;AACA,KAZA;;AAcA;AACA,IAAA,SAfA,qBAeA,CAfA,EAeA;AACA,WAAA,YAAA,GAAA,CAAA;AACA,KAjBA;AAkBA,IAAA,cAlBA,0BAkBA,CAlBA,EAkBA;AACA,WAAA,iBAAA,GAAA,CAAA;AACA,KApBA;;AAqBA;AACA,IAAA,aAtBA,yBAsBA,MAtBA,EAsBA;AACA,UAAA,MAAA,IAAA,CAAA,EACA,OAAA,iBAAA;AACA,UAAA,MAAA,IAAA,CAAA,EACA,OAAA,gBAAA;AACA,aAAA,QAAA;AACA,KA5BA;;AA6BA;AACA,IAAA,aA9BA,yBA8BA,MA9BA,EA8BA;AACA,UAAA,MAAA,IAAA,CAAA,EACA,OAAA,IAAA;AACA,UAAA,MAAA,IAAA,CAAA,EACA,OAAA,OAAA;AACA,KAnCA;;AAoCA;AACA,IAAA,WArCA,uBAqCA,KArCA,EAqCA;AACA,UAAA,GAAA,GAAA,CAAA,KAAA,GAAA,CAAA,EAAA,OAAA,CAAA,CAAA,EAAA,OAAA,CAAA,GAAA,EAAA,GAAA,CAAA;AACA,aAAA,GAAA,CAAA,QAAA,GAAA,OAAA,CAAA,uBAAA,EAAA,GAAA,CAAA;AACA,KAxCA;;AAyCA;AACA,IAAA,WA1CA,yBA0CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MADA,GACA,CAAA,KAAA,CAAA,WAAA,GAAA,CAAA,IAAA,KAAA,CAAA,QAAA,GAAA,CADA,EAEA;;AAFA;AAAA,uBAGA,KAAA,CAAA,GAAA,CACA,yDACA,MADA,GACA,SADA,GAEA,KAAA,CAAA,QAFA,GAEA,cAFA,GAGA,KAAA,CAAA,QAHA,GAGA,eAHA,GAIA,KAAA,CAAA,gBAJA,GAIA,aAJA,GAKA,KAAA,CAAA,cALA,GAKA,eALA,GAMA,KAAA,CAAA,aANA,GAMA,mBANA,GAOA,KAAA,CAAA,aAPA,GAOA,oBARA,CAHA;;AAAA;AAGA,gBAAA,QAHA;AAaA,gBAAA,KAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;;AAbA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA;AAxDA,GARA;AAkEA,EAAA,QAAA,EAAA,CACA;AACA;AACA;AACA;AACA;AALA,GAlEA;AAyEA,EAAA,IAzEA,kBAyEA;AACA,WAAA;AACA,MAAA,OAAA,EAAA,EADA;AAEA,MAAA,OAAA,EAAA,EAFA;AAGA,MAAA,QAAA,EAAA,EAHA;AAIA,MAAA,gBAAA,EAAA,EAJA;AAKA,MAAA,aAAA,EAAA,EALA;AAMA,MAAA,aAAA,EAAA,EANA;AAOA,MAAA,cAAA,EAAA,CAPA;AAQA,MAAA,QAAA,EAAA,CAAA,CARA;AASA,MAAA,QAAA,EAAA,EATA;AAUA,MAAA,WAAA,EAAA,CAVA;AAWA,MAAA,YAAA,EAAA,IAXA;AAYA,MAAA,iBAAA,EAAA,IAZA;AAaA,MAAA,QAAA,EAAA,OAAA,CAAA,WAAA,EAbA;AAcA,MAAA,OAAA,EAAA,UAdA;AAeA,MAAA,cAAA,EAAA;AAfA,KAAA;AAiBA,GA3FA;;AA4FA;AACA,EAAA,OA7FA,qBA6FA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,cAAA,MADA,GACA,CAAA,MAAA,CAAA,WAAA,GAAA,CAAA,IAAA,MAAA,CAAA,QAAA,GAAA,CADA;AAAA;AAAA,qBAEA,KAAA,CAAA,GAAA,CACA,yDAAA,MAAA,GAAA,SAAA,GAAA,MAAA,CAAA,QAAA,GAAA,gCADA,CAFA;;AAAA;AAEA,cAAA,QAFA;AAKA,cAAA,MAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA;AAnGA,CAAA","sourcesContent":["<template>\r\n  <div class=\"contenBody\">\r\n    <!-- header của content  -->\r\n    <div class=\"contentHeader\">\r\n      <!-- nút thêm  -->\r\n      <button\r\n        title=\"Thêm mới cửa hàng\"\r\n        class=\"contentHeaderButton\"\r\n        @click=\"btnAddOnClick\"\r\n      >\r\n        <div class=\"iconHeader\">\r\n          <div class=\"iconAdd\"></div>\r\n        </div>\r\n        <div class=\"iconText\">Thêm mới</div>\r\n      </button>\r\n      <!-- <ms-button class=\"contentHeaderButton\" :bgcolor=\"bg-color\" :icon=\"iconAdd\">\r\n        Thêm mới\r\n      </ms-button> -->\r\n      <!-- nhân bản  -->\r\n      <button\r\n        title=\"Nhân bản thêm một cửa hàng nữa\"\r\n        class=\"contentHeaderButton\"\r\n      >\r\n        <div class=\"iconHeader\">\r\n          <div class=\"iconDupplicate\"></div>\r\n        </div>\r\n        <div class=\"iconText\">Nhân bản</div>\r\n      </button>\r\n      <!-- sửa  -->\r\n      <button title=\"Chỉnh sửa thông tin cửa hàng\" class=\"contentHeaderButton\">\r\n        <div class=\"iconHeader\">\r\n          <div class=\"iconEdit\"></div>\r\n        </div>\r\n        <div class=\"iconText\">Sửa</div>\r\n      </button>\r\n      <!-- xóa  -->\r\n      <button title=\"Xóa cửa hàng\" class=\"contentHeaderButton\" @click=\"btnDeleteOnClick\">\r\n        <div class=\"iconHeader\">\r\n          <div class=\"iconDelete\"></div>\r\n        </div>\r\n        <div class=\"iconText\">Xóa</div>\r\n      </button>\r\n      <Popup\r\n        :isHidePopup=\"isHidePopupParent\" \r\n        @outIsHidePopup=\"outIsHidePopup\"\r\n        :product=\"product\"\r\n      />\r\n      <!-- nạp  -->\r\n      <button class=\"contentHeaderButton\">\r\n        <div class=\"iconHeader\">\r\n          <div class=\"iconImport\"></div>\r\n        </div>\r\n        <div class=\"iconText\">Nạp</div>\r\n      </button>\r\n    </div>\r\n\r\n    <div class=\"gridTable\">\r\n      <table class=\"tableContent\">\r\n        <thead>\r\n          <tr class=\"filter\">\r\n            <th width=\"4%\">\r\n              <input type=\"checkbox\" >\r\n            </th>\r\n            <th width=\"10%\">\r\n              SKU\r\n              <div class=\"filterField\">\r\n                <div class=\"iconSearch\">*</div>\r\n                <input\r\n                  class=\"searchField\"\r\n                  @keyup.enter=\"getPaginate\"\r\n                  v-model.trim=\"enterSKU\"\r\n                />\r\n              </div>\r\n            </th>\r\n            <th width=\"15%\">\r\n              Tên hàng hóa\r\n              <div class=\"filterField\">\r\n                <div class=\"iconSearch\">*</div>\r\n                <input\r\n                  class=\"searchField\"\r\n                  @keyup.enter=\"getPaginate\"\r\n                  v-model.trim=\"enterProductName\"\r\n                />\r\n              </div>\r\n            </th>\r\n            <th width=\"25%\">\r\n              Nhóm hàng hóa\r\n              <div class=\"filterField\">\r\n                <div class=\"iconSearch\">*</div>\r\n                <input class=\"searchField\" \r\n                @keyup.enter=\"getPaginate\"\r\n                  v-model.trim=\"enterCategory\"\r\n                />\r\n              </div>\r\n            </th>\r\n            <th width=\"8%\">\r\n              Đơn vị tính\r\n              <div class=\"filterField\">\r\n                <div class=\"iconSearch\">*</div>\r\n                <input class=\"searchField\"\r\n                @keyup.enter=\"getPaginate\"\r\n                  v-model.trim=\"enterUnitName\"\r\n                />\r\n              </div>\r\n            </th>\r\n            <th width=\"10%\" >\r\n              Giá bán TB\r\n              <div class=\"filterField\">\r\n                <div class=\"iconSearch\">≥</div>\r\n                <input class=\"searchField\"\r\n                @keyup.enter=\"getPaginate\"\r\n                  v-model.number=\"enterSellPrice\"\r\n                  style=\"text-align:right\"\r\n                />\r\n              </div>\r\n            </th>\r\n            <th width=\"15%\">\r\n              Hiển thị trên màn hình bán hàng\r\n              <div class=\"filterField\">\r\n                <select style=\"width: 100%;height: 38px\">\r\n                  <option value=\"2\" selected>Tất cả</option>\r\n                  <option value=\"1\">Có</option>\r\n                  <option value=\"0\">Không</option>\r\n                </select>\r\n              </div>\r\n            </th>\r\n            <th width=\"12%\">\r\n              Trạng thái\r\n              <div class=\"filterField\">\r\n                <select style=\"width: 100%;height: 38px\">\r\n                  <option value=\"2\">Tất cả</option>\r\n                  <option value=\"1\">Đang hoạt động</option>\r\n                  <option value=\"0\">Ngừng hoạt động</option>\r\n                </select>\r\n              </div>\r\n            </th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr\r\n            class=\"tableRow\"\r\n            v-for=\"(product, index) in products\"\r\n            :key=\"index\"\r\n            :class=\"{ hightlight: isActive == index }\"\r\n            @click=\"rowOnClick(product, index)\"\r\n          >\r\n            <td>\r\n              <div class=\"cell\">\r\n                <input type=\"checkbox\" v-model=\"checked\" :value=\"product.productID\">\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <div class=\"cell\">\r\n                {{ product.sku }}\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <div class=\"cell\" id=\"productName\">\r\n                {{ product.productName }}\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <div class=\"cell\">\r\n                {{ product.categoryName }}\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <div class=\"cell\">\r\n                {{ product.unitName }}\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <div class=\"cell\" style=\"text-align: right; padding-right: 5px\">\r\n                {{ formatPrice(product.sellPrice) }}\r\n              </div>\r\n            </td>\r\n            <td>\r\n              <div class=\"cell\">{{convertIsShow(product.isShow)}}</div>\r\n            </td>\r\n            <td>\r\n              <div class=\"cell\">{{convertStatus(product.status)}}</div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n    <!-- Component chi tiết  -->\r\n    <Details :isHide=\"isHideParent\" @outIsHide=\"outIsHide\" />\r\n    <div class=\"contentFooter\">\r\n      <div class=\"contentFooterLeft\">\r\n        <button class=\"buttonPage\">\r\n          <div class=\"firstPage\"></div>\r\n        </button>\r\n        <button class=\"buttonPage\">\r\n          <div class=\"prevPage\"></div>\r\n        </button>\r\n        <div class=\"contentFooterPaging\">\r\n          <a>Trang </a>\r\n          <input\r\n            type=\"text\"\r\n            class=\"currentPage\"\r\n            value=\"1\"\r\n            v-model.number=\"currentPage\"\r\n            @keyup.enter=\"getPaginate\"\r\n          />\r\n          <a> trên </a>\r\n          <div class=\"totalPage\">{{products.length/typePage}}</div>\r\n        </div>\r\n        <button class=\"buttonPage\">\r\n          <div class=\"nextPage\"></div>\r\n        </button>\r\n        <button class=\"buttonPage\">\r\n          <div class=\"lastPage\"></div>\r\n        </button>\r\n        <button class=\"buttonPage\">\r\n          <div class=\"refresh\"></div>\r\n        </button>\r\n        <select class=\"typePage\" v-model=\"typePage\">\r\n          <option value=\"5\">5</option>\r\n          <option value=\"25\" selected>25</option>\r\n          <option value=\"50\">50</option>\r\n          <option value=\"100\">100</option>\r\n        </select>\r\n      </div>\r\n\r\n      <div class=\"contentFooterRight\">\r\n        <!-- todo proc cần trả về số bản ghi thỏa mãn và số trang thỏa mãn -->\r\n        <a>Hiển thị 1- {{ products.length }} trên {{ products.length }} kết quả</a>\r\n        <!-- <p>Các input đã check: {{ checked }}</p> -->\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as axios from \"axios\";\r\nimport service from \"@/data.js\";\r\nimport Details from \"./ProductListDetails.vue\";\r\nimport Popup from \"../popup/PopupDelete\";\r\nexport default {\r\n  name: \"Product\",\r\n  props: {\r\n  },\r\n  components: {\r\n    Details,\r\n    Popup\r\n  },\r\n  methods: {\r\n    /**Chọn dòng  */\r\n     rowOnClick(product ,index) {\r\n      this.isActive = index;\r\n      this.product = product;\r\n    },\r\n    /**Mở form */\r\n    btnAddOnClick() {\r\n      this.isHideParent = !this.isHideParent;\r\n    },\r\n    btnDeleteOnClick(){\r\n      this.isHidePopupParent = !this.isHidePopupParent;\r\n    },\r\n\r\n    /**Đóng form */\r\n    outIsHide(e) {\r\n      this.isHideParent = e;\r\n    },\r\n    outIsHidePopup(e) {\r\n      this.isHidePopupParent = e;\r\n    },\r\n    /**format trạng thái */\r\n    convertStatus(status){\r\n      if(status == 0)\r\n      return \"Ngừng hoạt động\";\r\n      if(status == 1)\r\n      return \"Đang hoạt động\";\r\n      return \"Tất cả\";\r\n    },\r\n    /**format hiển thị */\r\n    convertIsShow(isShow){\r\n      if(isShow == 0)\r\n      return \"Có\";\r\n      if(isShow ==1)\r\n      return \"Không\";\r\n    },\r\n    /**format tiền */\r\n    formatPrice(value) {\r\n        let val = (value/1).toFixed(2).replace('.', ',')\r\n        return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \".\")\r\n    },\r\n    /**API phân trang + filter */\r\n    async getPaginate(){\r\n      var offset = (this.currentPage - 1)*this.typePage + 1 ;\r\n      // var sellPriceConverted = this.enterSellPrice.convertSellPrice();\r\n    const response = await axios.get(\r\n      \"http://localhost:55810/api/Products/Paginate?offset=\"\r\n      + offset +\"&limit=\"\r\n      +this.typePage+\"&productSKU=\"\r\n      +this.enterSKU+\"&productName=\"\r\n      +this.enterProductName+\"&sellPrice=\"\r\n      +this.enterSellPrice+\"&productUnit=\"\r\n      +this.enterUnitName+\"&productCategory=\"\r\n      +this.enterCategory+\"&isShow=2&status=2\"\r\n    );\r\n    this.products = response.data;\r\n    }\r\n  },\r\n  computed:{\r\n    // formatInputPrice(enterSellPrice){\r\n    //   if(enterSellPrice < 0)\r\n    //   enterSellPrice = 0;\r\n    //   return enterSellPrice;\r\n    // }\r\n  },\r\n  data() {\r\n    return {\r\n      product:{},\r\n      checked: [],\r\n      enterSKU:\"\",\r\n      enterProductName:\"\",\r\n      enterCategory:\"\",\r\n      enterUnitName:\"\",\r\n      enterSellPrice: 0,\r\n      isActive: -1,\r\n      typePage:25,\r\n      currentPage:1,\r\n      isHideParent: true,\r\n      isHidePopupParent:true,\r\n      products: service.getProducts(),\r\n      allMode: \"allPages\",\r\n      checkBoxesMode: \"always\",\r\n    };\r\n  },\r\n  /**API ban đầu */\r\n  async created() {\r\n    var offset = (this.currentPage - 1)*this.typePage + 1 ;\r\n    const response = await axios.get(\r\n      \"http://localhost:55810/api/Products/Paginate?offset=\"+ offset +\"&limit=\"+ this.typePage+\"&sellPrice=0&isShow=2&status=2\"\r\n    );\r\n    this.products = response.data;\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.filter .filterField {\r\n  height: 46px;\r\n  width: 100%;\r\n  padding: 4px;\r\n  display: flex;\r\n}\r\n.iconSearch {\r\n  border: 1px solid #9e9e9e;\r\n  border-radius: 4px;\r\n  margin: 1px;\r\n  height: 100%;\r\n  width: 40px;\r\n  line-height: 40px;\r\n  text-align: center;\r\n  background-color: #ffffff;\r\n}\r\n.searchField {\r\n  margin: 1px;\r\n  height: 100%;\r\n  width: calc(100% - 40px);\r\n  border-radius: 4px;\r\n  border: 1px solid #9e9e9e;\r\n}\r\n.hightlight {\r\n  \r\n  background-color: #dfe3e8;\r\n  border-left: 1px solid #dfe3e8;\r\n\r\n}\r\n/* .contenBody{} */\r\n\r\n.contentHeader {\r\n  height: 40px;\r\n  background-color: #2b3173;\r\n  position: relative;\r\n  display: flex;\r\n  justify-content: flex-start;\r\n  align-items: center;\r\n}\r\n\r\n.contentHeader .contentHeaderButton .iconText {\r\n  color: #ffffff;\r\n  margin-left: 5px;\r\n}\r\n\r\n.contentHeader .contentHeaderButton .iconHeader {\r\n  align-items: center;\r\n  justify-content: center;\r\n  padding: 3px;\r\n}\r\n\r\n.currentPage {\r\n  width: 45px;\r\n}\r\n.typePage {\r\n  width: 75px;\r\n}\r\na {\r\n  margin: 10px;\r\n}\r\n\r\n</style>"],"sourceRoot":"src/views/product"}]}