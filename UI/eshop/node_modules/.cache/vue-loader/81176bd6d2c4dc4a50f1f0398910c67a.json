ton class=\"buttonPage\">\r\n          <div class=\"nextPage\"></div>\r\n        </button>\r\n        <button class=\"buttonPage\">\r\n          <div class=\"lastPage\"></div>\r\n        </button>\r\n        <button class=\"buttonPage\">\r\n          <div class=\"refresh\"></div>\r\n        </button>\r\n        <select class=\"typePage\" v-model=\"typePage\">\r\n          <option value=\"5\">5</option>\r\n          <option value=\"25\" selected>25</option>\r\n          <option value=\"50\">50</option>\r\n          <option value=\"100\">100</option>\r\n        </select>\r\n      </div>\r\n\r\n      <div class=\"contentFooterRight\">\r\n        <a>Hiển thị 1- 10 trên {{ products.length }} kết quả</a>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as axios from \"axios\";\r\nimport Details from \"./ProductListDetails.vue\";\r\nexport default {\r\n  name: \"Product\",\r\n  props: {},\r\n  components: {\r\n    Details,\r\n    // MsButton\r\n  },\r\n  methods: {\r\n    onSelectionChanged({ products }) {\r\n      const data = products[1];\r\n\r\n      this.showEmployeeInfo = !!data;\r\n      this.selectedRowNotes = data && data.Notes;\r\n      this.selectedRowPicture = data && data.Picture;\r\n    },\r\n    btnAddOnClick() {\r\n      this.isHideParent = !this.isHideParent;\r\n    },\r\n    outIsHide(e) {\r\n      this.isHideParent = e;\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      typePage: 25,\r\n      currentPage: 1,\r\n      isHideParent: true,\r\n      allMode: \"allPages\",\r\n      checkBoxesMode: \"always\",\r\n      products: [\r\n        {\r\n          productID: \"00000000-0000-0000-0000-000000000000\",\r\n          productName: \"Res01\",\r\n          sku: \"Res 01\",\r\n          status: 0,\r\n          categoryCode: 0,\r\n          categoryName: \"\",\r\n          buyPrice: 0,\r\n          sellPrice: 0,\r\n          unitCode: 1,\r\n          unitName: \"Đôi\",\r\n          isShow: 1,\r\n          description: \"